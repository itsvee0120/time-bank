{
  "expo": {
    "name": "Time Bank Router Fix",
    "slug": "time-bank-router-fix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/HOUREUM.png",
    "scheme": "timebankfix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nviol.timebankrouterfix"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/HOUREUM.png",
        "backgroundImage": "./assets/images/HOUREUM.png",
        "monochromeImage": "./assets/images/HOUREUM.png"
      },
      "package": "com.nviol.timebankrouterfix",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/HOUREUM.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/HOUREUM.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#041b0c",
          "dark": {
            "backgroundColor": "#041b0c"
          }
        }
      ],
      "expo-sqlite",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    }
  }
}
